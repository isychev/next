{
  "extends": ["tslint:recommended", "tslint-react", "tslint-eslint-rules"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "no-debugger": false,
    "jsx-no-multiline-js": false
  }
}
